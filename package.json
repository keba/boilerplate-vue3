{
  "author": "keba",
  "bin": {},
  "dependencies": {
    "@mdi/font": "7.4.47",
    "ajv": "8.17.1",
    "ajv-formats": "3.0.1",
    "axios": "1.8.4",
    "lodash": "4.17.21",
    "pinia": "3.0.2",
    "vue": "3.5.13",
    "vue-cookies": "1.8.6",
    "vue-router": "4.5.0",
    "vuetify": "3.8.1"
  },
  "description": "website",
  "devDependencies": {
    "@eslint/js": "9.24.0",
    "@playwright/test": "1.51.1",
    "@stylistic/eslint-plugin-js": "4.2.0",
    "@types/node": "22.14.1",
    "@vitejs/plugin-vue": "5.2.3",
    "@vue/test-utils": "2.4.6",
    "eslint": "9.24.0",
    "eslint-formatter-junit": "8.40.0",
    "eslint-plugin-vue": "10.0.0",
    "globals": "16.0.0",
    "jsdom": "26.0.0",
    "luxon": "3.6.1",
    "rollup-plugin-summary": "3.0.1",
    "sass-embedded": "1.86.3",
    "unplugin-vue-components": "28.4.1",
    "vite": "6.2.6",
    "vite-plugin-vuetify": "2.1.1",
    "vue-eslint-parser": "10.1.3"
  },
  "engines": {
    "node": ">=22.14.0",
    "npm": ">=10.9.2",
    "yarn": ">=100.100.100"
  },
  "homepage": "https://github.com/keba/boilerplate-vue3",
  "license": "MIT License",
  "name": "boilerplate-vue3",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/keba/boilerplate-vue3.git"
  },
  "scripts": {
    "build-checked-hotfix": "(npm run eslint)&&(npm run build-hotfix)",
    "build-checked-production": "(npm run eslint)&&(npm run build-production)",
    "build-checked-staging": "(npm run eslint)&&(npm run build-staging)",
    "build-checked-tests": "(npm run eslint)&&(npm run build-tests)",
    "build-hotfix": "(npm run build-remove-old-files)&&(vite build --mode production)",
    "build-production": "(npm run build-remove-old-files)&&(vite build --mode production)",
    "build-remove-old-files": "(rm -rf dist)",
    "build-staging": "(npm run build-remove-old-files)&&(vite build --mode staging)",
    "build-tests": "(npm run build-remove-old-files)&&(vite build --mode test)",
    "check-circleci": "circleci config validate",
    "check-outdated": "npm outdated",
    "dev": "vite",
    "dev-debug": "(npm run eslint)&&(node --inspect ./node_modules/.bin/vite)",
    "dev-force": "(rm -rf node_modules/.vite)&&(vite --force)",
    "dev-checked": "(npm run eslint)&&(npm run dev)",
    "dev-checked-host": "(npm run eslint)&&(npm run dev-host)",
    "dev-checked-no-oudated": "(npm run eslint-no-outdated)&&(npm run dev)",
    "dev-with-dev-tools": "(export LOAD_DEVTOOLS=true;npm run dev-checked)",
    "dev-host": "vite --host",
    "preview": "vite preview",
    "preview-hotfix": "(npm run eslint)&&(npm run build-hotfix)&&(vite preview)",
    "preview-production": "(npm run eslint)&&(npm run build-production)&&(vite preview)",
    "preview-staging": "(npm run eslint)&&(npm run build-staging)&&(vite preview)",
    "preview-tests": "(npm run eslint)&&(npm run build-tests)&&(vite preview)",
    "tests-start": "(npm run check-outdated)&&(npm run eslint)&&(playwright install --with-deps)&&(export CHECK_SCREENSHOTS=TRUE;playwright test)",
    "tests-start-no-screenshots": "(npm run check-outdated)&&(npm run eslint)&&(playwright install --with-deps)&&(export CHECK_SCREENSHOTS=FALSE;playwright test)",
    "tests-start-raw": "(npm run check-outdated)&&(npm run eslint)&&(playwright install --with-deps)&&(export CHECK_SCREENSHOTS=TRUE;playwright test --project=raw)",
    "tests-start-unauthenticated": "(npm run check-outdated)&&(npm run eslint)&&(playwright install --with-deps)&&(export CHECK_SCREENSHOTS=TRUE;playwright test --project=unauthenticated)",
    "tests-start-with-build": "(npm run check-outdated)&&(npm run eslint)&&(npm run build-tests)&&(playwright install --with-deps)&&(export CHECK_SCREENSHOTS=TRUE;playwright test)",
    "eslint": "(npm run check-outdated)&&(npm run check-circleci)&&(./node_modules/eslint/bin/eslint.js -c eslint.config.mjs src/** tests/**/*.js playwright.config.js *.mjs)",
    "eslint-fix": "(npm run check-outdated)&&(npm run check-circleci)&&(./node_modules/eslint/bin/eslint.js -c eslint.config.mjs --fix src/** tests/**/*.js playwright.config.js *.mjs)",
    "eslint-no-outdated": "./node_modules/eslint/bin/eslint.js -c eslint.config.mjs src/** tests/**/*.js playwright.config.js *.mjs"
  },
  "type": "module",
  "version": "1.0.0"
}
